<div class="container">
    <div class="row">

{{#each albums}}

<div class="col-lg-4 artist-container">
        
          <img  src="{{this.images.0.url}}" alt="" class="artist-img">
        
        <div class="artist-info-container">
          <p>{{this.name}}</p>
          <a href="/tracks/{{this.id}}">View Albums</a>
        </div>
</div>



{{/each}}

    </div>


</div>






